[Unit]
Description = Python script to start rosbridge service
After = network.target

[Service]
Type = simple
ExecStart = /usr/bin/node /home/robot/Madox/madox_ws/src/ros2-web-bridge/bin/rosbridge.js
User = robot
Restart = on-failure
RestartSec = 5
TimeoutStartSec = infinity

[Install]
WantedBy = multi-user.target 
